{
  "restartable": "rs",
  "ignore": [],
  "verbose": true,
  "execMap": {
      "": "node",
      "js": "node --harmony"
  },
  "events": {
      "restart": "osascript -e 'display notification \"App restarted due to:\n'$FILENAME'\" with title \"nodemon\"'"
  },
  "watch": [
      "server/"
  ],
  "env": {
      "NODE_ENV": "development"
  },
  "ext": "js json",
  "legacy-watch": false
}